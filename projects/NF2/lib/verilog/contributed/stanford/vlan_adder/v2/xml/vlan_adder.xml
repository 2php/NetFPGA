<?xml version="1.0" encoding="UTF-8"?>
<nf:module xmlns:nf="http://www.NetFPGA.org/NF2_register_system" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.NetFPGA.org/NF2_register_system NF2_register_system.xsd ">
	<nf:name>vlan_adder</nf:name>
	<nf:prefix>vlan_adder</nf:prefix>
	<nf:location>udp</nf:location>
	<nf:description>VLAN adder register and constants</nf:description>
	<nf:blocksize>64</nf:blocksize>
	<nf:registers>
		<nf:register>
			<nf:name>vlan_tag</nf:name>
			<nf:description>VLAN tag and Add Modify Remove specifying register</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
	</nf:registers>
	<nf:constants>
		<nf:constant>
			<nf:name>vlan_ctrl_word</nf:name>
			<nf:width>8</nf:width>
			<nf:value>0x42</nf:value>
		</nf:constant>
		<nf:constant>
			<nf:name>vlan_ethertype</nf:name>
			<nf:width>16</nf:width>
			<nf:value>0x8100</nf:value>
		</nf:constant>
	</nf:constants>
	<nf:types>
	</nf:types>
</nf:module>
